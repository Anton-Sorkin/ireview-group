<div class="admin-users-container">

	<div class="admin-users-frame">

		<span class="back-to-main">
			<a href="/admin" class="admin-links">Back to Admin Dash</a>
		</span>

		{{#each users}}
			<form action="/admin/users/{{_id}}" method="post" class="del-user-form">
				<span class="user-admin-li">{{username}}</span>
				<button type="submit" class="delete-user-btn">
					Delete User
				</button>
			</form>
		{{/each}}
	</div>

</div>